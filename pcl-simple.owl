        <rdfs:isDefinedBy rdf:resource="http://purl.obolibrary.org/obo/iao.owl"/>
        <rdfs:label>definition</rdfs:label>
        <rdfs:label xml:lang="en">definition</rdfs:label>
    </owl:AnnotationProperty>
    


